{
  "manifest_version": 3,
  "name": "YouTube Video Timestamp Repeater",
  "version": "1.1.5",
  "description": "Repeat a specific timeline of YouTube videos.",
  "permissions": ["activeTab"],
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
